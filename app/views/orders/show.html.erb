<h1>Order Details:</h1>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm">
      <h2>
        Order # <%= @order.id %>
      </h2>
      <p><%= @order.user.email %></p>
      <p><%= @order.status %></p>
      <p>Total Price:<%= number_to_euro @order.total_price %></p>
    </div><!-- end col -->
  </div><!-- end row -->
</div>


  